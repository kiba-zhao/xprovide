{
  "name": "xprovide",
  "version": "0.2.1",
  "description": "module provider tools",
  "main": "index.js",
  "types": "typings/index.d.ts",
  "scripts": {
    "docs": "jsdoc -c conf.json",
    "build": "tsc -d",
    "build:w": "tsc -d -w",
    "lint": "eslint --fix .",
    "check": "npm run build && npm run lint",
    "test": "npm run check && jest --coverage --env=node --runInBand",
    "watch": "jest --watch"
  },
  "files": [
    "typings",
    "package.json",
    "lib",
    "xboot.js",
    "index.js"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kiba-zhao/xprovide.git"
  },
  "keywords": [
    "xboot",
    "provide",
    "provider"
  ],
  "author": "kiba.x.zhao",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/kiba-zhao/xprovide/issues"
  },
  "homepage": "https://github.com/kiba-zhao/xprovide#readme",
  "devDependencies": {
    "@types/jest": "^26.0.20",
    "@types/lodash": "^4.14.167",
    "@types/node": "^14.14.20",
    "eslint": "^7.17.0",
    "eslint-config-egg": "^9.0.0",
    "eslint-plugin-jest": "^24.1.3",
    "husky": "^4.3.7",
    "jest": "^26.6.3",
    "jsdoc": "^3.6.6",
    "typescript": "^4.1.3"
  },
  "dependencies": {
    "is-class": "0.0.9",
    "lodash": "^4.17.20"
  },
  "husky": {
    "hooks": {
      "pre-push": "npm test"
    }
  },
  "jest": {
    "moduleNameMapper": {
      "^@/(.*)$": "<rootDir>/$1"
    },
    "collectCoverageFrom": [
      "lib/**/*.js",
      "xboot.js"
    ]
  }
}
